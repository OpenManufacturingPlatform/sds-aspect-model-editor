{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noStrictGenericChecks": true,
    "strictNullChecks": false,
    "paths": {
      "@bame/api": ["libs/api/src/index.ts"],
      "@bame/app/*": ["apps/bame/src/app/*"],
      "@bame/aspect-exporter": ["libs/aspect-exporter/src/index.ts"],
      "@bame/cache": ["libs/cache/src/index.ts"],
      "@bame/connection": ["libs/connection/src/index.ts"],
      "@bame/editor": ["libs/editor/src/index.ts"],
      "@bame/instantiator": ["libs/instantiator/src/index.ts"],
      "@bame/meta-model": ["libs/meta-model/src/index.ts"],
      "@bame/mx-graph": ["libs/mx-graph/src/index.ts"],
      "@bame/rdf/*": ["libs/rdf/src/lib/*"],
      "@bame/settings-dialog": ["libs/settings-dialog/src/index.ts"],
      "@bame/shared": ["libs/shared/src/index.ts"],
      "@bame/vocabulary": ["libs/vocabulary/src/index.ts"]
    },
    "typeRoots": ["node_modules/@types", "node_modules/@typed-mxgraph"],
    "types": ["node", "jest"],
    "lib": ["es2018", "dom"]
  },
  "exclude": ["node_modules", "tmp"]
}
