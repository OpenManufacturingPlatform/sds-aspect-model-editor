{
  "name": "ame",
  "author": "Bosch",
  "description": "Used to design and edit BAMM specific aspect models.",
  "productName": "Aspect-Model-Editor",
  "version": "3.0.0",
  "main": "main.js",
  "build": {
    "appId": "com.bci.ame",
    "artifactName": "ame-ui.zip",
    "win": {
      "target": "zip",
      "icon": "./apps/bame/src/assets/img/bosch_ic_product_aspect_model_editor_general.ico"
    },
    "directories": {
      "output": "./electron"
    }
  },
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "lint": "nx run-many --all --target=lint --exclude=bame-e2e",
    "start": "ng serve --configuration development",
    "build": "ng build --configuration development",
    "build:stats": "ng build --configuration production --stats-json",
    "build:prod": "ng build --configuration production",
    "format:check-apps": "prettier --config ./.prettierrc --list-different \"./apps/bame/src/{app,assets}/**/*{.ts,.js,.json,.html,.scss}\"",
    "format:check-libs": "prettier --config ./.prettierrc --list-different \"./libs/**/*{.ts,.js,.json,.html,.scss}\"",
    "prettier:apps": "prettier --config ./.prettierrc --write \"./apps/bame/src/{app,assets}/**/*{.ts,.js,.json,.html,.scss}\" \"./apps/bame-e2e/src/integration/**/*{.ts,.js,.json,.html,.scss}\"",
    "prettier:libs": "prettier --config ./.prettierrc --write \"./libs/**/*{.ts,.js,.json,.html,.scss}\"",
    "prettier:env": "prettier --config ./.prettierrc --write \"./environments/**/*{.ts,.js,.json,.html,.scss}\"",
    "analyse": "webpack-bundle-analyzer dist/bame/stats.json",
    "update-imprint": "ts-node ./set-imprint-details.mjs",
    "version:set": "replace-in-files --string=DEV-SNAPSHOT --replacement=$VERSION ./apps/bame/src/assets/imprint-details.json",
    "copy:assets": "cpx \"./node_modules/@bci-web-core/common-styles/{fonts,img}/*.*\" \"/apps/bame/src/assets\"",
    "doc:app": "compodoc -p tsconfig.app.json -t --hideGenerator -d \\\"dist/docs/app\\\"",
    "test-e": "electron-packager .",
    "electron": "electron-builder build --win",
    "electron:ci": "electron-builder build --win",
    "electron-start-dev": ".\\node_modules\\electron\\dist\\electron.exe . --dev",
    "electron-run-win": ".\\node_modules\\electron\\dist\\electron.exe .",
    "electron-start-win": "ng build --configuration development --base-href .\\ && npm run electron-run-win",
    "electron-start-win-prod": "ng build --configuration production --base-href .\\ && npm run electron-run-win",
    "electron-start-unix": "ng build --configuration development --base-href ./ && ./node_modules/.bin/electron .",
    "electron-start-unix-prod": "ng build --configuration production --base-href ./ && ./node_modules/.bin/electron .",
    "help": "nx help",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:nx-check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "test": "nx run-many --all --target=test",
    "test:coverage": "nx run-many --all --target=test --coverage",
    "test:headless": "npm run test:headless:app",
    "cypress:clean": "rimraf 'cypress/reports' && rimraf 'coverage' && rimraf '.nyc_output'",
    "cypress:ci": "nx e2e bame-e2e",
    "cypress": "npm run cypress:clean && nx e2e bame-e2e",
    "cypress:open": "npm run cypress:clean && npx cypress open"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts"
    ],
    "include": [
      "apps/bame/**/*.ts",
      "libs/**/*.ts"
    ],
    "exclude": [
      "cypress/",
      "apps/bame-e2e",
      "apps/bame/**/*.spec.ts",
      "libs/**/*.spec.ts"
    ],
    "all": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.4",
    "@angular/cdk": "^12.2.4",
    "@angular/common": "^12.2.4",
    "@angular/compiler": "^12.2.4",
    "@angular/core": "^12.2.4",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.2.4",
    "@angular/material": "^12.2.4",
    "@angular/platform-browser": "^12.2.4",
    "@angular/platform-browser-dynamic": "^12.2.4",
    "@angular/router": "^12.2.4",
    "@bci-web-core/common-styles": "^2.12.0",
    "@bci-web-core/core": "^12.0.0",
    "@bci-web-core/material-theme": "^12.0.0",
    "@bci-web-core/web-components": "^4.8.1",
    "@juggle/resize-observer": "^3.3.1",
    "electron-localshortcut": "^3.2.1",
    "electron-pdf-window": "^1.0.12",
    "electron-squirrel-startup": "^1.0.0",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.4.6",
    "is-electron": "^2.2.0",
    "locale-codes": "^1.3.1",
    "loglevel": "^1.7.1",
    "mxgraph": "^4.2.2",
    "mxgraph-factory": "^4.2.0",
    "n3": "~1.6.4",
    "ngx-electron": "^2.2.0",
    "ngx-toastr": "^14.2.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "rxjs": "^7.3.0",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.0.0",
    "urn-lib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^12.1.2",
    "@angular-devkit/build-angular": "^12.2.4",
    "@angular-eslint/builder": "^12.3.1",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.3.1",
    "@angular/cli": "^12.2.4",
    "@angular/compiler-cli": "^12.2.4",
    "@angular/language-service": "^12.2.0",
    "@angularclass/hmr": "^3.0.0",
    "@briebug/cypress-schematic": "^5.0.1",
    "@compodoc/compodoc": "^1.1.14",
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@nrwl/angular": "^12.9.0",
    "@nrwl/cli": "^12.9.0",
    "@nrwl/cypress": "^12.9.0",
    "@nrwl/eslint-plugin-nx": "^12.9.0",
    "@nrwl/jest": "^12.9.0",
    "@nrwl/linter": "^12.9.0",
    "@nrwl/nx-cloud": "^12.3.10",
    "@nrwl/tao": "^12.9.0",
    "@nrwl/workspace": "^12.9.0",
    "@rdfjs/data-model": "^1.3.3",
    "@typed-mxgraph/typed-mxgraph": "^1.0.4",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^27.0.1",
    "@types/n3": "^1.4.4",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "cpx": "^1.5.0",
    "cypress": "^6.9.1",
    "electron": "10.1.3",
    "electron-builder": "^22.11.7",
    "electron-packager": "^15.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "events": "^3.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^27.2.0",
    "jest-junit": "^12.2.0",
    "jest-preset-angular": "^9.0.7",
    "mocha": "^9.1.1",
    "mochawesome": "^6.2.2",
    "ng-packagr": "^12.2.1",
    "ngx": "^2.4.0",
    "ngx-build-plus": "^12.2.0",
    "nx": "^12.9.0",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "replace-in-files-cli": "^1.0.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "~4.3.5",
    "webpack": "^5.52.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    ",> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ],
  "browser": {
    "fs": true,
    "os": true,
    "path": true
  }
}
